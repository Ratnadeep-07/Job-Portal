package master.dao;

import java.sql.*;
import master.utility.DBConnection;

public class ApplyJobDao {
 public void apply(String user,String job,String date){
 try{
 Connection con = DBConnection.getConnection();
 PreparedStatement ps = con.prepareStatement("INSERT INTO applyjob VALUES(?,?,?)");
 ps.setString(1,user);
 ps.setString(2,job);
 ps.setString(3,date);
 ps.executeUpdate();
 }
 catch(Exception e)
 { 
	 e.printStackTrace(); 
  }
 }
}
